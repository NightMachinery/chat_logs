[06:04:31] <mknod> # lines=(xxx xxxxxxxxxxxxxxxxxx xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx); for ((i=0; i<5000; i++)); do printf '%s\n' "${lines[RANDOM % 3]}"; done | xargs awk 'BEGIN {print ARGC-1}'
